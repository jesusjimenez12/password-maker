import random
caracteres = "+-&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

longitud = int (input ("longitud de la contraseña:"))
contra = ""
for i in range(longitud):
    letra = random.choice(caracteres)
    contra = contra + letra
    
print ("tu contraseña es :" , contra)
